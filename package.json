{
  "name": "flux-app",
  "version": "0.0.0",
  "private": true,
  "main": "start.js",
  "scripts": {
    "build": "gulp",
    "watch": "gulp dev",
    "dev": "NODE_PATH=. npm run watch & npm run mon",
    "mon": "NODE_PATH=. nodemon index.js --watch lib/app/* lib/server/* -e js,jsx",
    "lint": "semistandard --verbose | snazzy",
    "start": "NODE_PATH=. node index.js"
  },
  "dependencies": {
    "babel": "^5.4.7",
    "body-parser": "^1.6.4",
    "compression": "^1.5.1",
    "cookie-parser": "^1.3.3",
    "csurf": "^1.6.3",
    "debug": "^2.0.0",
    "express": "^4.3.2",
    "express-state": "^1.2.0",
    "fluxible": "^0.5.0",
    "fluxible-addons-react": "^0.1.6",
    "fluxible-plugin-fetchr": "^0.3.0",
    "fluxible-router": "^0.2.0",
    "react": "^0.13.0",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.1.6"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "del": "^1.2.1",
    "gulp": "^3.9.0",
    "gulp-size": "^1.3.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "nodemon": "^1.2.1",
    "reactify": "^1.1.1",
    "semistandard": "^7.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1"
  },
  "browserify": {
    "transform": ["babelify"]
  },
  "semistandard": {
    "ignore": [
      "./build/*",
      "./node_modules/*"
    ]
  }
}
